nstanceof Scene_Battle) {\n    const actorCommandWindow = SceneManager._scene._actorCommandWindow;\n\n    if (actorCommandWindow && actorCommandWindow.active && actorCommandWindow.index() >= 0) {\n        const currentCommand = actorCommandWindow.currentData();\n\n        if (currentCommand) {\n            let nameToDisplay = currentCommand.name; // e.g., \"\\I[128]Shield bash\"\n            let actualSymbol = currentCommand.symbol; // This will be 'singleSkill'\n\n            // Regex to remove \\i[NN] or \\I[NN] from the beginning, and any space after.\n            // The 'i' flag at the end makes the regex case-insensitive.\n            nameToDisplay = nameToDisplay.replace(/^\\\\i\\[\\d+\\]\\s*/i, \"\");\n            nameToDisplay = nameToDisplay.trim(); // Clean any remaining whitespace\n\n            // Check against the CORRECT symbol 'singleSkill' (camelCase)\n            if (actualSymbol === 'singleSkill') { // <<<< THE CRITICAL CHANGE IS HERE\n                return nameToDisplay; // Display the cleaned name\n            } else {\n                // If it's another command like \"Attack\", \"Guard\", display nothing.\n                // You could put debug output here too if needed for other symbols, but for now, empty is fine.\n                return \"\"; // Or: \"Symbol: \" + actualSymbol (for other commands)\n            }\n\n        } else {\n            return \"\";\n        }\n    } else if (actorCommandWindow && actorCommandWindow.active) {\n        return \"\";\n    } else if (actorCommandWindow) {\n        return \"\";\n    } else {\n        return \"\";\n    }\n}\nreturn \"Not in Battle or Not Battle Scene\";","Display":"Skill Name","Type":7}},"Type":0,"X":551,"Y":400}]}],"Map":0}